#include <stdio.h>
void main()
{
    int marks;
    printf("enter your marks:");
    scanf("%d",&marks);
    if(marks>=85 && marks<=100)
    {
        printf("Grade A");
    }
    else if(marks>=70 && marks<=84)
    {
        printf("Grade B");
    }
    else if(marks>=55 && marks<=69)
    {
        printf("Grade C");
    }
    else if (marks>=49 && marks<=54)
    {
        printf("Grade D");
    }
    else 
    {
        printf("Grade F");
    }
}
